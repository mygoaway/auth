    {
      "type": "timeseries",
      "title": "Login Attempts (by channel & result)",
      "id": 10,
      "gridPos": {
        "x": 0,
        "y": 27,
        "w": 12,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(auth_login_attempts_total{application=\"auth-service\"}[1m])) by (channel, result)",
          "legendFormat": "{{channel}} - {{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
